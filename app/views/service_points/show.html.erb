<h1><%= @service_point.city.name + " - " + @service_point.service.name %></h1>

<p><strong><%= raw @service_point.address %></strong></p>
<p>Contacts: <%= raw @service_point.contacts %></p>

<%= link_to @service_point.service.name, [@service_point, @service_point.service] %>

<% content_for :extra do %>
    <%= image_tag @service_point.map %>
<% end %>